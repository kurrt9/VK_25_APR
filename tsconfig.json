{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["cypress"]
  },
  "include": ["cypress/**/*.ts", "cypress/**/*.d.ts"]
}